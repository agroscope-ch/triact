citHeader("To cite triact in publications use:")

bibentry(
  bibtype = "Article",
  title = "{triact} package for R: Analyzing the lying behavior of cows from accelerometer data.",
  author = c(as.person("Michael Simmler"), as.person("Stijn Pieter Brouwers")),
  journal = "In review",
  year = 2023,
  textVersion =
  paste("Michael Simmler, Stijn Pieter Brouwers (2023).",
        "triact package for R: Analyzing the lying behavior of cows from accelerometer data.",
        "In review")
)
